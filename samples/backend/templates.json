{
  "$schema": "https://unpkg.com/jatg/templates.schema.json",
  "templates": [
    {
      "name": "module",
      "sourcePaths": ["./templates"],
      "outputPath": "./src/modules/%name.plural.kebabCase%",
      "variables": [
        {
          "variable": "name",
          "name": "Entity Name",
          "preprocessing": ["trim", "singular", "kebabCase"]
        }
      ]
    }
  ]
}
